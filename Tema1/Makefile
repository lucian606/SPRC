CLIENT = rpc_db_client
SERVER = rpc_db_server

SOURCES_CLNT.cpp = 
SOURCES_CLNT.h = 
SOURCES_SVC.cpp = 
SOURCES_SVC.h = 
SOURCES.x = rpc_db.x

TARGETS_SVC.cpp = rpc_db_svc.cpp rpc_db_server.cpp rpc_db_xdr.cpp 
TARGETS_CLNT.cpp = rpc_db_clnt.cpp rpc_db_client.cpp rpc_db_xdr.cpp 
TARGETS = rpc_db.h rpc_db_xdr.cpp rpc_db_clnt.cpp rpc_db_svc.cpp rpc_db_client.cpp rpc_db_server.cpp

OBJECTS_CLNT = $(SOURCES_CLNT.cpp:%.cpp=%.o) $(TARGETS_CLNT.cpp:%.cpp=%.o)
OBJECTS_SVC = $(SOURCES_SVC.cpp:%.cpp=%.o) $(TARGETS_SVC.cpp:%.cpp=%.o)
# Compiler flags 

CFLAGS += -g
LDLIBS += -lnsl -ltirpc
RPCGENFLAGS = 

# Targets 

all : $(CLIENT) $(SERVER)

$(TARGETS) : $(SOURCES.x) 
	rpcgen $(RPCGENFLAGS) $(SOURCES.x)

$(OBJECTS_CLNT) : $(SOURCES_CLNT.cpp) $(SOURCES_CLNT.h) $(TARGETS_CLNT.cpp) 

$(OBJECTS_SVC) : $(SOURCES_SVC.cpp) $(SOURCES_SVC.h) $(TARGETS_SVC.cpp) 

$(CLIENT) : $(OBJECTS_CLNT) 
	$(LINK.cpp) -o $(CLIENT) $(OBJECTS_CLNT) $(LDLIBS) 

$(SERVER) : $(OBJECTS_SVC) 
	$(LINK.cpp) -o $(SERVER) $(OBJECTS_SVC) $(LDLIBS)

clean:
	 $(RM) core $(TARGETS) $(OBJECTS_CLNT) $(OBJECTS_SVC) $(CLIENT) $(SERVER)

run_client:
	./rpc_db_client 127.0.0.1

run_server:
	./rpc_db_server